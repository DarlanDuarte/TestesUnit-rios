{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier", "react-hooks", "@typescript-eslint", "import"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error", {}, { "endOfLine": "auto" }]
  }
}
